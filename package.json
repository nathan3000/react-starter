{
  "name": "boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "classnames": "^2.2.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "tailwindcss": "^1.0.5"
  },
  "scripts": {
    "build:css": "postcss src/index.tailwind.css -o src/index.css",
    "watch:css": "postcss src/index.tailwind.css -o src/index.css -w",
    "start": "npm run watch:css & rescripts start",
    "build": "npm run build:css && rescripts build",
    "test": "rescripts test",
    "eject": "rescripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@rescripts/cli": "0.0.11",
    "@rescripts/rescript-env": "0.0.10",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "autoprefixer": "^9.6.1",
    "eslint-plugin-prettier": "^3.1.0",
    "postcss-cli": "^6.1.3",
    "typescript": "^3.5.3",
    "@storybook/react": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9"
  },
  "rescripts": [
    "env"
  ]
}
